class Module {
  int x;
  int y;
  int w;
  int h;
  String name;

  Module(int xIn, int yIn, int wIn, int hIn, String nameIn) {
    x = xIn;
    y = yIn;
    w = wIn;
    h = hIn;
    name = nameIn;
  }
  void update() {
  }
  void makeButtons() {
    cp5.addButton(name).setPosition(x+1, y+1)
      .setSize(w-1, h-1);
  }
  void display() {
    fill(153);
    stroke (255);
    rect(x, y, w, h);
  }
}


