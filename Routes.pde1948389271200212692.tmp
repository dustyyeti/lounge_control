void Router(String go, String msg) {  // //<>//
  println("<--- Router(go, msg: " 
    +go
    +", "
    +msg
    );

  String updateText = "";

  if (go == "rgbR" || go == "rgbG" || go == "rgbB"){
    println("HIHIHIHIHIHIHIHIHIHIHIHIHIHIHIHIHI");
    cPick = color(rgbR,rgbG,rgbB,255);
  }
  
  switch(go) {

  case "g3": // GUI group 3
    break;
  case "lounge_count":
    updateText = (lounge_count +" lounges zoned");
    break;
  case "connect_B":
    if (!CONNECTED) {
      try {
        if (Serial.list()[0].isEmpty()) {
          port= new Serial(this, Serial.list()[0], 9600);
        }
        updateText = "Wahooo! connected @ "+Serial.list()[0];
        CON_CHANGED = true;
      } 
      catch (Exception e) {
        updateText = "Booo, can't find a valid serial connection!  :(";
      }
    } else {
      updateText = "Ok, I'll disconnect...";
      CON_CHANGED=true;
    }
    break;
  case "out":
    println("OUT CASE START COM");
    updateText = msg;
    if (CONNECTED) {
      updateText = "dance "+updateText; 
      WritePort(msg);
    } else {
      updateText = "demo " + updateText;
    }
    break;
  default:
    return;
  }
  UpdateUI(updateText);
  console.scroll(1);
}

